
  <% for(var i = 0; i < course.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= course[i].courseName %></td>
        <td><%= course[i].duration %></td>
        <td><%= course[i].fees %></td>
        <td>
            <a href="/update-course?id=<%= course[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id="<%=course[i]._id%>">
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a>
        </td>
    </tr>
    <% } %>